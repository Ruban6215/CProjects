#include <stdio.h>
#include <conio.h>
int main()
{
    int age;
    printf("Enter your Age: ");
    scanf("%d",&age);
    if (age>=18)
        printf("You are eligible for voting as your age is %d",age);
    else
        printf("You are not eligible for voting since you are %d",age);
    getch();
}

