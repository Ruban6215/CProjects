#include <stdio.h>
#include <conio.h>
//Created by Ruban on 11.10.2022

void main() {
    int a;
    printf("Enter your subject mark: ");
    scanf("%d", &a);
    if (a >= 90) {
        printf("The grade is 'O'");
    } else if (a >= 80) {
        printf("The grade is 'A'");
    } else if (a >= 70) {
        printf("The grade is 'B;");
    } else if (a >= 60) {
        printf("The grade is 'C'");
    } else if (a >= 50) {
        printf("The grade is 'D'");
    } else if (a >= 35) {
        printf("The grade is 'E'");
    } else if (a < 35) {
        printf("Sorry, You have not passed (F Grade) ");
    }
    getch();
}

